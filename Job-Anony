    weapons = {"keys","weapon_arc_atmcard","idcard","weapon_physcannon","gmod_camera","gmod_tool","weapon_physgun","weapon_fists","brh_ci"},
    command = "mafieux",
    max = 10,
    salary = 30,
    admin = 0,
    vote = false,
    hasLicense = false,
    candemote = false,
    PlayerSpawn = function(ply)
        ply:SetMaxHealth(100)
        ply:SetHealth(100)
        ply:SetArmor(0) end,
		customCheck = function(ply) return CLIENT or
    table.HasValue({"VIP","user","Helpeur","Modérateur-Test","Modérateur","admin","Chef-Staff","superadmin"}, ply:GetNWString("usergroup"))
    end,
	CustomCheckFailMsg = "Métier FULL",
    PlayerDeath = function(ply, weapon, killer)
        ply:teamBan()
        ply:changeTeam(GAMEMODE.DefaultTeam, true)
        DarkRP.notifyAll(0, 4, "Un mafieux est mort")
    end
})

