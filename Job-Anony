TEAM_GARAGE = DarkRP.createJob("Garagiste", {
    color = Color(83, 95, 99, 255),
    model = {"models/player/mechanic.mdl"},
    description = [[Vous etes garagiste. Vous reparé les véhicule]],
    weapons = {"keys","idcard","vc_wrench","weapon_physcannon","gmod_camera","gmod_tool","weapon_physgun","weapon_fists","weapon_arc_atmcard","brh_ci"},
    command = "garage",
    max = 2,
    salary = 30,
    admin = 0,
    vote = false,
    hasLicense = false,
    candemote = false,
    PlayerSpawn = function(ply)
        ply:SetMaxHealth(100)
        ply:SetHealth(100)
        ply:SetArmor(0) end,
		customCheck = function(ply) return CLIENT or
    table.HasValue({"VIP","user","Helpeur","Modérateur-Test","Modérateur","admin","Chef-Staff","superadmin"}, ply:GetNWString("usergroup"))
    end,
	CustomCheckFailMsg = "Métier FULL",
    PlayerDeath = function(ply, weapon, killer)
        ply:teamBan()
        ply:changeTeam(GAMEMODE.DefaultTeam, true)
        DarkRP.notifyAll(0, 4, "Un Garagiste est mort")
    end
})

